{
  "name": "me",
  "runtime": "nodejs4.3",
  "description": "${project} - ${stage}: me",
  "customName": false,
  "customRole": false,
  "handler": "me/handler.handler",
  "timeout": 120,
  "memorySize": 128,
  "authorizer": {},
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [
    {
      "path": "authentication/me",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "CUSTOM",
      "authorizerFunction": "authorize",
      "apiKeyRequired": false,
      "requestParameters": {
      },
      "requestTemplates": "$${apiPrincipalIdMapRequestTemplate}",
      "responses": {
        "400": {
          "statusCode": "400"
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": {
            "application/json": ""
          }
        }
      }
    },
    {
      "path": "authentication/me",
      "method": "OPTIONS",
      "type": "MOCK",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${apiCorsRequestTemplate}",
      "responses": "$${apiCorsOptionsResponse}"
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "serverless-authentication-boilerplate",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}",
    "REDIRECT_CLIENT_URI": "${redirectClientURI}",
    "TOKEN_SECRET": "${tokenSecret}",
    "PROVIDER_FACEBOOK_ID": "${providerFacebookId}",
    "PROVIDER_FACEBOOK_SECRET": "${providerFacebookSecret}",
    "PROVIDER_GOOGLE_ID": "${providerGoogleId}",
    "PROVIDER_GOOGLE_SECRET": "${providerGoogleSecret}",
    "PROVIDER_MICROSOFT_ID": "${providerMicrosoftId}",
    "PROVIDER_MICROSOFT_SECRET": "${providerMicrosoftSecret}",
    "PROVIDER_CUSTOM_GOOGLE_ID": "${providerCustomGoogleId}",
    "PROVIDER_CUSTOM_GOOGLE_SECRET": "${providerCustomGoogleSecret}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  },
  "custom": {
    "cors": {
      "allowOrigin": "*",
      "allowHeaders": ["Content-Type", "X-Amz-Date", "Authorization", "X-Api-Key"]
    },
    "optimize": {
      "exclude": ["aws-sdk"],
      "transforms": [
        {
          "name": "babelify",
          "opts": {
            "presets": [
              "es2015"
            ]
          }
        }
      ]
    }
  }
}